{
  "options": {
    "ver": "1.0.0",
    "inspectConflicts": false,
    "semicolons": true,
    "freeze": [],
    "ts": true
  },
  "app": {
    "environmentsAllowingSeedData": "",
    "seedData": false,
    "name": "api",
    "description": "Feathers api server",
    "src": "src",
    "packager": "npm@>= 3.0.0",
    "providers": [
      "rest",
      "socketio"
    ]
  },
  "services": {
    "todos": {
      "name": "todos",
      "nameSingular": "todo",
      "subFolder": "",
      "fileName": "todos",
      "adapter": "nedb",
      "path": "/todos",
      "isAuthEntity": false,
      "graphql": true
    }
  },
  "hooks": {
    "process-todo": {
      "fileName": "process-todo",
      "camelName": "processTodo",
      "ifMulti": "n",
      "multiServices": [],
      "singleService": "todos"
    }
  },
  "connections": {
    "nedb": {
      "database": "nedb",
      "adapter": "nedb",
      "connectionString": "nedb://../data"
    }
  }
}
