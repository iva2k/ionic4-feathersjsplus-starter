{
  "options": {
    "ver": "1.0.0",
    "inspectConflicts": false,
    "semicolons": true,
    "freeze": [],
    "ts": true
  },
  "app": {
    "environmentsAllowingSeedData": "development,test",
    "seedData": true,
    "name": "api",
    "description": "Feathers api server",
    "src": "src",
    "packager": "npm@>= 3.0.0",
    "providers": [
      "rest",
      "socketio"
    ]
  },
  "services": {
    "todos": {
      "name": "todos",
      "nameSingular": "todo",
      "subFolder": "",
      "fileName": "todos",
      "adapter": "nedb",
      "path": "/todos",
      "isAuthEntity": false,
      "graphql": true
    },
    "users": {
      "name": "users",
      "nameSingular": "user",
      "subFolder": "",
      "fileName": "users",
      "adapter": "nedb",
      "path": "/users",
      "isAuthEntity": true,
      "requiresAuth": true,
      "graphql": true
    }
  },
  "hooks": {
    "process-todo": {
      "fileName": "process-todo",
      "camelName": "processTodo",
      "ifMulti": "n",
      "multiServices": [],
      "singleService": "todos"
    },
    "gravatar": {
      "fileName": "gravatar",
      "camelName": "gravatar",
      "ifMulti": "n",
      "multiServices": [],
      "singleService": "users"
    },
    "populate-user": {
      "fileName": "populate-user",
      "camelName": "populateUser",
      "ifMulti": "n",
      "multiServices": [],
      "singleService": "todos"
    }
  },
  "connections": {
    "nedb": {
      "database": "nedb",
      "adapter": "nedb",
      "connectionString": "nedb://../data"
    }
  },
  "authentication": {
    "strategies": [
      "local",
      "google",
      "facebook"
    ],
    "entity": "users"
  }
}
